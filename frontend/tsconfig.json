{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": false,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "esNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
, "src/store/index.js"  ],
  "exclude": [
    "src/**/*.js"
  ]
}
